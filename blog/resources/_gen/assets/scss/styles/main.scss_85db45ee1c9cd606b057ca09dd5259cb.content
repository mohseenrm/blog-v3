html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;font-size:100%;font:inherit;vertical-align:baseline}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after{content:"";content:none}q:before,q:after{content:"";content:none}table{border-collapse:collapse;border-spacing:0}html,body{width:100%}#timeline-content{display:flex;background:#fff;padding:0% 5%;height:100%;border-radius:5px}.nav-bar{display:flex;justify-content:center;align-items:center}.content{display:flex;flex-direction:column;background:#ffe259;background:-webkit-linear-gradient(to top,#ffa751,#ffe259);background:linear-gradient(to top,#ffa751,#ffe259);padding:0% 3% 3%;height:100%}body{font-size:14px;line-height:1.5}h1,h2,h3,h4,h5,h6{font-family:playfair display,serif;font-weight:700;font-style:italic}h1{font-size:3rem}h2{font-size:2.5rem}h3{font-size:2rem}h4{font-size:1.5rem}h5,h6{font-size:1rem}div,p,span{font-family:source sans pro,sans-serif;font-size:1rem}article{text-align:justify}@media only screen and (min-device-width:320px) and (max-device-width:812px){h1{font-size:2rem}h2{font-size:1.8rem}h3{font-size:1.5rem}h4{font-size:1.3rem}h5,h6{font-size:1rem}}.timeline-item{padding:3em 2em 2em;position:relative;color:rgba(0,0,0,.7);border-left:2px solid rgba(255,218,11,.787)}.timeline-item p{font-size:1rem}.timeline-item::before{content:attr(date-is);position:absolute;left:2em;font-weight:700;top:1.1em;display:block;font-weight:700;font-size:.9rem;letter-spacing:1px}.timeline-item::after{width:15px;height:15px;display:block;top:1em;position:absolute;left:-11px;border-radius:15px;content:'';border:2px solid rgba(255,218,11,.787);background:#fff}.timeline-item:last-child{border-image:linear-gradient(to bottom,rgba(255,218,11,0.787) 60%,rgba(255,218,11,0)) 1 100%}.link{color:#000;font-size:2rem;font-weight:700;letter-spacing:1px;text-decoration:none;background-image:-webkit-linear-gradient(to right,#ff9966,#ff5e62);background-image:linear-gradient(to right,#ff9966,#ff5e62);background-repeat:no-repeat;background-size:100% .2em;background-position:0 80%;transition:background-size .25s ease-in,color .25s ease-in,font-weight .25s ease-in}.link:hover{color:#fff;background-size:100% 88%;font-weight:100}.logo{display:flex;justify-content:flex-end;position:fixed;top:0;right:3%}svg text#mm{stroke-dasharray:800;animation:fill_logo 3s cubic-bezier(0.11,0.5,0.83,0.67);opacity:1}svg rect#border{stroke-dasharray:800;stroke-linejoin:round;stroke-linecap:round;stroke-width:15;animation:fill_border 3s cubic-bezier(0.11,0.5,0.83,0.67);opacity:1}@keyframes fill_logo{0%{opacity:0;fill:none;stroke-dashoffset:800}30%{opacity:1}90%{fill:transparent}100%{fill:#000;stroke-dashoffset:0}}@keyframes fill_border{0%{opacity:0;stroke-dashoffset:800;stroke-width:1}30%{opacity:1;stroke-width:1}85%{stroke-width:2}100%{stroke-dashoffset:0;stroke-width:2}}@media only screen and (min-device-width:320px) and (max-device-width:812px){.logo{margin:2%}.logo>svg#logo{width:10%;margin-top:2.8%}.link{font-size:1.3rem}}